# Git Commands Shortcuts
alias gitvr = "git --version"
alias gitpl = "git pull origin"
alias gitph = "git push origin"
alias gitphft = "git push origin --follow-tags"
alias gitcm = "git commit -m"
alias gitck = "git checkout"
alias gitmg = "git merge"
alias gittg = "git tag"

# NPM and NodeJS Commands Shortcuts
alias npmvr = "npm --version"
alias npmup = "npm update"
alias npmst = "npm init"
alias nodevr = "node --version"

# PHP Commands Shortcuts
alias phpvr = "php --version"
alias phpsv = "php -S localhost:8000"

# Java Commands Shortcuts
alias javavr = "java --version"
